{
	"name": "baadal",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"clean": "rm -rf .next; rm -rf node_modules/@generated",
		"dev": "node boot/boot.js --dev",
		"next:dev": "next dev",
		"next:build": "next build",
		"fix": "prettier --write",
		"g": "run-s g:*",
		"g:graphql": "graphql-codegen --config codegen.yml",
		"g:options": "NODE_PATH='.' ts-node scripts/generate-option-resolvers.ts",
		"g:client": "yarn clean; prisma generate",
		"lint": "next lint",
		"prepare": "run-s g:{options,client}",
		"start": "next start"
	},
	"prettier": "@barelyhuman/prettier-config",
	"dependencies": {
		"@graphql-codegen/typescript-generic-sdk": "^2.3.4",
		"@prisma/client": "3.9.2",
		"@twind/next": "^1.0.9",
		"@types/graphql-fields": "^1.3.4",
		"@types/node": "^17.0.18",
		"@types/toastify-js": "^1.11.0",
		"@urql/core": "^2.4.3",
		"@urql/exchange-auth": "^0.1.7",
		"class-validator": "^0.13.2",
		"got": "^12.0.1",
		"graphql": "15.3.0",
		"graphql-fields": "^2.0.3",
		"graphql-helix": "^1.11.0",
		"graphql-middleware": "6.0.9",
		"lodash": "^4.17.21",
		"next": "12.0.10",
		"prisma": "3.9.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"reflect-metadata": "^0.1.13",
		"swr": "^1.2.2",
		"toastify-js": "^1.11.2",
		"ts-node": "^10.5.0",
		"twind": "^0.16.16",
		"type-graphql": "^1.1.1",
		"typescript": "^4.5.5",
		"urql-generic-requester": "^0.0.1"
	},
	"devDependencies": {
		"@arkweid/lefthook": "^0.7.7",
		"@barelyhuman/prettier-config": "^0.1.0",
		"@graphql-codegen/cli": "2.6.2",
		"@graphql-codegen/introspection": "2.1.1",
		"@graphql-codegen/typescript": "2.4.5",
		"@graphql-codegen/typescript-operations": "2.3.2",
		"@types/lodash": "^4.14.179",
		"@types/react": "^17.0.39",
		"@types/simple-oauth2": "^4.1.1",
		"eslint": "8.9.0",
		"eslint-config-next": "12.0.10",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.1",
		"tsconfig-paths": "^3.13.0",
		"typegraphql-prisma": "^0.18.5"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"ts-node": {
		"experimentalResolverFeatures": true,
		"require": [
			"tsconfig-paths/register"
		]
	}
}
